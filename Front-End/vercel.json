{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "pnpm run build",
    "outputDirectory": "dist",
    "installCommand": "pnpm install --no-frozen-lockfile --shamefully-hoist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}